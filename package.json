{
  "name": "thinkfree-node",
  "version": "1.0.0",
  "description": "Blog site built on Nodejs with SSR for blog content and react for user login and commenting",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run build:dev\" \"npm run start-server:dev\"",
    "build": "npm run build:prod && npm run start-server:prod",
    "build:dev": "webpack --config webpack-dev.config.js",
    "build:prod": "webpack --config webpack-prod.config.js",
    "start-server:dev": "NODE_ENV=development nodemon src/server.js --exec 'node -r dotenv/config'",
    "start-server:prod": "NODE_ENV=production forever start forever-config.json"
  },
  "author": "Friedman Smith",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^6.11.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.1",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "forever": "^4.0.1",
    "install": "^0.13.0",
    "lightbox-appco": "^1.0.4",
    "menu-appco": "^1.1.1",
    "nodemailer": "^6.6.3",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login-component": "^0.9.3",
    "react-router-dom": "^5.2.0",
    "reactform-appco": "^1.0.14",
    "router-express": "^1.5.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^6.2.0",
    "nodemon": "^2.0.12",
    "style-loader": "^3.2.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  }
}
