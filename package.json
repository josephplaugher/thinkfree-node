{
  "name": "thinkfree-node",
  "version": "1.0.0",
  "description": "Blog site built on Nodejs with SSR for blog content and react for user login and commenting",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run build:dev\" \"npm run start-server:dev\"",
    "build": "npm run build:prod && npm run start-server:prod",
    "build:dev": "webpack --config webpack-dev.config.js",
    "build:prod": "webpack --config webpack-prod.config.js",
    "start-server:dev": "NODE_ENV=development nodemon src/server.js --exec 'node -r dotenv/config'",
    "start-server:prod": "NODE_ENV=production forever start forever-config.json"
  },
  "author": "Friedman Smith",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^4.4.2",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "concurrently": "^3.6.1",
    "dotenv": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "forever": "^0.15.3",
    "install": "^0.12.1",
    "nodemailer": "^4.7.0",
    "pg": "^7.4.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-google-login-component": "^0.9.3",
    "react-router-dom": "^4.3.1",
    "reactform-appco": "^2.0.0",
    "router-express": "^1.5.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  }
}
